{
	"useTabs": true,
	"tabWidth": 3,
	"semi": true,
	"singleQuote": false,
	"trailingComma": "all",
	"objectWrap": "collapse",
	"arrowParens": "avoid",
	"printWidth": 80,
	"proseWrap": "always",
	"plugins": ["prettier-plugin-svelte"],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
