{
	"vars": {
		"width": 100,
		"height": "{width}",
		"n-spokes": 16,
		"cx": "{(/ width 2)}",
		"cy": "{(/ height 2)}",
		"spoke-length": "{(* width 0.75)}"
	},
	"attrs": {
		"viewBox": "0 0 {width} {height}"
	},
	"children": [
		{
			"for_each": {
				"variable": "i",
				"in": { "start": 0, "end": "{n-spokes}" }
			},
			"do": {
				"tag": "line",
				"vars": {
					"theta": "{(* (/ i n-spokes) (pi))}",
					"dx": "{(* (/ spoke-length 2) (cos theta))}",
					"dy": "{(* (/ spoke-length 2) (sin theta))}"
				},
				"attrs": {
					"x1": "{(+ cx dx)}",
					"x2": "{(- cx dx)}",
					"y1": "{(+ cy dy)}",
					"y2": "{(- cy dy)}",
					"stroke": "hsl\\({(* (/ i n-spokes) 360)}, 100%, 50%\\)"
				}
			}
		}
	]
}
